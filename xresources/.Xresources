! Xft settings -----------------------------------------------------------------
!DPI scales everything
Xft.dpi: 125
Xft.antialias: true
Xft.lcdfilter: lcddefault
Xft.autohint: false
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb

XTerm*background: #2b2b2b
XTerm*foreground: #e7e7e7
XTerm*pointerColor: #16A085
XTerm*faceName: Fixed
XTerm*faceSize: 11


! Rofi

!rofi.color-enabled: true
!rofi.color-window: #2b303b, #2b303b, #65737e
!rofi.color-normal: #2b303b, #eff1f5, #2b303b, #65737e, #eff1f5
!rofi.color-active: #2b303b, #a3be8c, #2b303b, #65737e, #a3be8c
!rofi.color-urgent: #2b303b, #ebcb8b, #2b303b, #65737e, #ebcb8b
!
!rofi.bw: 0
!rofi.font: Roboto Mono 14
!rofi.lines: 16
!rofi.separator-style: none
!rofi.padding: 20
!
!rofi.run-command: zsh -c "{cmd}"
